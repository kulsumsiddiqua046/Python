# Greatest of two numbers
x=10
y=20
if x > y:
  print("x is greatest")
else:
  print("y is greatest")


# Greatest among three numbers
a=int(input("Enter a number:"))
b=int(input("Enter a number:"))
c==int(input("Enter a number:"))
if a > b:
  if a > c:
    print("x is greatest:")
  else:
    print("z is greatest:")
else: b > c:
    print("y is greatest:")
